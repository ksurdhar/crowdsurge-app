<h2>New Ticket</h2>

<%= render "layouts/errors" %>

<form action="<%= tickets_url %>" method="post">
  <%= auth_token_input %>

  <label>
    Event
    <input type="text" name="ticket[event]" value="<%= @ticket.event %>" id="title">
  </label>

  <label>
    Description
    <input type="text" name="ticket[description]" value="<%= @ticket.description %>" id="url">
  </label>

  <input type="submit" value="Create New Ticket">
</form>

